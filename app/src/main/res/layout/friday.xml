<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="1"
    android:background="#F0F0F0">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:paddingBottom="25dp"
        android:background="@color/green">
        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/rightbtn5"
            android:layout_marginLeft="15dp"
            android:background="@drawable/left" />

        <TextView
            android:text="@string/Fri_text"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:textSize="30sp"
            android:id="@+id/ttdHeader"
            android:singleLine="false"
            android:paddingLeft="50dp"
            android:textColor="@color/white"
            android:gravity="center_vertical|bottom"
            android:layout_gravity="center" />

        <Button
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/leftbtn2"
            android:background="@null"
            android:clickable="false"/>
    </LinearLayout>


    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="bottom">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/fr2"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/btn"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="7:30AM - 9:00AM\n\nBreakfast"
                android:textAllCaps="false"
                android:textColor="#5A6C64"/>
            <Button
                android:id="@+id/fr3"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:background="@drawable/microphone"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="9:00AM - 10:30AM\n\nConservation\nNancy Knowlton, Eduardo Eizirik, Louis Bernatchez"
                android:textAllCaps="false"
                android:textColor="#5A6C64"/>
            <Button
                android:id="@+id/fr4"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="10:30AM - 11:00AM\n\nCoffee Break"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/coffee"/>
            <Button
                android:id="@+id/fr5"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="11:00AM - 12:30PM\n\nGlobal Initiatives\nJohn Kress, Wolfgang WÃ¤gele, Peter Hollingsworth"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/microphone" />
            <Button
                android:id="@+id/fri6"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="12:30PM - 2:00PM\n\nLunch"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/btn"/>
            <Button
                android:id="@+id/fr7"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nSupporting Laws to Protect Threatened Species"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>
            <Button
                android:id="@+id/fr8"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nPolicy and Regulatory Developments"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>

            <Button
                android:id="@+id/fr9"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nEducation and Barcoding"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>
            <Button
                android:id="@+id/fri10"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nAdvancing Knowledge of Terrestrial Biodiversity"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>
            <Button
                android:id="@+id/fr11"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nMolecular Evolution"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>
            <Button
                android:id="@+id/fr12"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="2:00PM - 3:30PM\n\nNext Generation Platforms and Analysis Pipelines 2"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@drawable/parallel"/>
            <Button
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:textAllCaps="false"
                android:text="3:30PM - 4:00PM\n\nCoffee Break"
                android:id="@+id/fr13"
                android:layout_marginBottom="2dp"
                android:paddingTop="10dp"
                android:textColor="#5A6C64"
                android:paddingLeft="120dp"
                android:gravity="top"
                android:background="@drawable/coffee"/>
            <Button
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:textAllCaps="false"
                android:text="4:00PM - 5:30PM\n\nFinal Plenary"
                android:id="@+id/fr14"
                android:layout_marginBottom="2dp"
                android:paddingTop="10dp"
                android:textColor="#5A6C64"
                android:paddingLeft="120dp"
                android:gravity="top"
                android:background="@drawable/microphone"
                />
            <Button
                android:id="@+id/fr15"
                android:layout_width="fill_parent"
                android:layout_height="100dp"
                android:layout_marginBottom="2dp"
                android:gravity="top"
                android:paddingLeft="120dp"
                android:paddingTop="10dp"
                android:text="6:30PM - 8:30PM\n\nSelf Organized Night in Guelph"
                android:textAllCaps="false"
                android:textColor="#5A6C64"
                android:background="@color/white" />
        </LinearLayout>
    </ScrollView>
</LinearLayout>